<!DOCTYPE html>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Car Dealership</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
  </head>
  <body>
    <h1>Welcome to the Dealership</h1>

    <table>
      <thead>
        <th>Make</th>
        <th>Model</th>
        <th>Year</th>
        <th>Price</th>
      </thead>
      @foreach(var car in Model)
        {
          <tr>
            <td>@car.GetMake()</td>
            <td>@car.GetModel()</td>
            <td>@car.GetYear()</td>
            <td>@car.GetPrice()</td>
          </tr>
        }
    </table>
  </body>
</html>
